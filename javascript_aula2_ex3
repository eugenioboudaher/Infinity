<body>
  <script>
    var idade = Number(prompt('Digite a idade:'))
    var preco = Number(prompt('Digite o preco:'))
    var preco_desc = preco * 0.9
    if(idade < 18){
      alert(`Desconto de 10% total R$ ${preco_desc.toFixed(2)}`)
    }else {
      alert(`Sem desconto, bvalor R$ ${preco}`)
    }
    

  </script>
  
</body>
</html>
