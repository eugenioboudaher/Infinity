<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <script>
    function somar(numero1) {
        return function(numero2){
            return numero1 + numero2
        }
    }
    const soma5 = somar(5)
    let result1 = soma5(10)
    let result11 = document.createElement('p')
    result11.textContent = result1
    document.body.appendChild(result11)
  </script>
</body>
</html>
