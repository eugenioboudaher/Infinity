<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <script>
    var num = Number(prompt('Digite um número'));
    let list_divisors = [];
    for(let i = 1; i <= num; i++){
      if (num % i === 0) {
        list_divisors.push(i);
     }
    }
    if (list_divisors.length <= 2){
      alert('Primo')
    }else {
      alert('Não primo')
    }
  </script>  
</body>
</html>
