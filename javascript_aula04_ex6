<style>
    #num {
        font-size: 2em;
    }
</style>
<body>
    
    <div id="num"></div>

    <script>
        let div_letter = document.querySelector('#num');
        let word = prompt('Digite a palavra');
        for (letter of word){
            if ('aeiou'.includes(letter)){
                continue
            }else {
                let div_elements = document.createElement('p');
                div_elements.textContent = letter;
                div_letter.appendChild(div_elements)
            }
            

        }


    </script>

</body>
